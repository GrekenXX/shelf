<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<plugin>
	<extension id="Boost Test Output Parser" name="Boost Test Output Parser" point="org.eclipse.cdt.core.ErrorParser">
		<errorparser class="org.eclipse.cdt.core.errorparsers.RegexErrorParser" id="org.eclipse.cdt.ui.Boost Test Output Parser" name="Boost Test Output Parser">
			<pattern description-expr="$3" eat-processed-line="true" file-expr="$1" line-expr="$2" regex="(.*)\((\d*)\): ((fatal )?error in &quot;.*&quot;:.*)" severity="Error"/>
			<pattern description-expr="$1" eat-processed-line="true" file-expr="" line-expr="" regex="\*\*\* (\d* failures detected in test suite &quot;.*&quot;)" severity="Error"/>
			<pattern description-expr="$3" eat-processed-line="true" file-expr="$1" line-expr="$2" regex="(.*)\((\d*)\): (last checkpoint)" severity="Info"/>
		</errorparser>
	</extension>
</plugin>
